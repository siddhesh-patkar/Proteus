   11                           ;Initialization code:
   12 F000:8E00 FF         [ 3] start:          lds #$FF
   13 F003:CE10 00         [ 3]                 ldx #$1000
   14                           
   15 F006:BDF8 BE         [ 6]                 jsr lcdinit
   16                           
   17                           ;Reset the graphics home address:
   18 F009:CC04 00         [ 3] main:           ldd #$0400                      
   19 F00C:BDF9 25         [ 6]                 jsr wrdouble
   20 F00F:8642            [ 2]                 ldaa #LCD_GFXHOME
   21 F011:BDF9 10         [ 6]                 jsr wrcmd
   22                           
   23                           ;Write the Labcenter logo to the LCD graphics area
   24                           ;This is performed in auto-write mode for maximum speed.
   25 F014:CC04 00         [ 3]                 ldd #$0400
   26 F017:BDF8 F8         [ 6]                 jsr wraddr
   27 F01A:86B0            [ 2]                 ldaa #LCD_AUTOWRITE
   28 F01C:BDF9 10         [ 6]                 jsr wrcmd
   29 F01F:18CE F0BE       [ 4]                 ldy #lxlogo
   30 F023:18A6 00         [ 5] loop1           ldaa 0,Y
   31 F026:BDF9 3F         [ 6]                 jsr awdata
   32 F029:1808            [ 4]                 iny
   33 F02B:188C F33E       [ 5]                 cpy #presents
   34 F02F:26F2 (F023)     [ 3]                 bne loop1
   35                           
   36 F031:BDF0 AF         [ 6]                 jsr delay
   37                           
   38                           ;Now the word 'presents'. The LCD is still in auto-write mode.
   39 F034:18A6 00         [ 5] loop2           ldaa 0,Y
   40 F037:BDF9 3F         [ 6]                 jsr awdata
   41 F03A:1808            [ 4]                 iny
   42 F03C:188C F4BE       [ 5]                 cpy #vsmlogo
   43 F040:26F2 (F034)     [ 3]                 bne loop2
   44                           
   45 F042:BDF0 AF         [ 6]                 jsr delay
   46                           
   47                           
   48                           ;Cancel auto-write mode, then re-start it at the top of the display:
   49 F045:BDF9 54         [ 6]                 jsr awreset
   50 F048:CC04 00         [ 3]                 ldd #$0400
   51 F04B:BDF8 F8         [ 6]                 jsr wraddr
   52 F04E:86B0            [ 2]                 ldaa #LCD_AUTOWRITE
   53 F050:BDF9 10         [ 6]                 jsr wrcmd
   54                           
   55                           ;Now the VSM logo is transferred:
   56 F053:18CE F4BE       [ 4]                 ldy #vsmlogo
   57 F057:18A6 00         [ 5] loop3           ldaa 0,Y
   58 F05A:BDF9 3F         [ 6]                 jsr awdata
   59 F05D:1808            [ 4]                 iny
   60 F05F:188C F8BE       [ 5]                 cpy #vsmlogoend
   61 F063:26F2 (F057)     [ 3]                 bne loop3
   62 F065:BDF9 54         [ 6]                 jsr awreset
   63                           
   64 F068:BDF0 AF         [ 6]                 jsr delay
   65                           
   66                           ;Finally, we scroll it off the screen by moving the GFXHOME address
   67                           ;downwards by one row (16 bytes) at a time.
   68 F06B:CC04 00         [ 3]                 ldd #$0400
   69 F06E:36              [ 3] loop4a          psha
   70 F06F:37              [ 3]                 pshb
   71 F070:BDF9 25         [ 6]                 jsr wrdouble
   72 F073:8642            [ 2]                 ldaa #LCD_GFXHOME
   73 F075:BDF9 10         [ 6]                 jsr wrcmd
   74 F078:CC03 E8         [ 3]                 ldd #1000
   75 F07B:8300 01         [ 4] loop4b          subd #1
   76 F07E:26FB (F07B)     [ 3]                 bne loop4b
   77 F080:33              [ 4]                 pulb
   78 F081:32              [ 4]                 pula
   79 F082:C300 10         [ 4]                 addd #16
   80 F085:1A83 0800       [ 5]                 cpd #$0800
   81 F089:26E3 (F06E)     [ 3]                 bne loop4a
   82                           
   83 F08B:BDF0 AF         [ 6]                 jsr delay
   84                           
   85                           ;Clear the screeen so that we are ready to start again.
   86                           ;The graphics screen occupies 1k of memory.
   87 F08E:CC04 00         [ 3]                 ldd #$0400
   88 F091:BDF8 F8         [ 6]                 jsr wraddr
   89 F094:86B0            [ 2]                 ldaa #LCD_AUTOWRITE
   90 F096:BDF9 10         [ 6]                 jsr wrcmd
   91 F099:18CE 0000       [ 4]                 ldy #0
   92 F09D:4F              [ 2] loop5           clra
   93 F09E:BDF9 3F         [ 6]                 jsr awdata
   94 F0A1:1808            [ 4]                 iny
   95 F0A3:188C 0400       [ 5]                 cpy #$0400
   96 F0A7:26F4 (F09D)     [ 3]                 bne loop5
   97 F0A9:BDF9 54         [ 6]                 jsr awreset
   98                           
   99                           ;And round we go again...
  100 F0AC:7EF0 09         [ 3]                 jmp main
