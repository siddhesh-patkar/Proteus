<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<CLIP CATEGORY="Breakout Peripherals" FILE="Arduino/Arduino Piezo Sounder Breakout Board.pdsclip" FAMILY="ARDUINO" DESC="Arduino Piezo Breakout Board" >
   <PERIPHERAL TYPE="Arduino Piezo" ELEMENT="LS1">

      <!-- Hardware Interface -->
      <INTERFACE OBJECT="GenericPiezo">
         <PIN NAME="IO" TYPE="DIGITAL">
            <TERMINAL ELEMENT="IO6"/>
         </PIN>
      </INTERFACE>

      <!-- Firmware Driver -->
      <DRIVER HEADER="Generic.h">
         <METHOD NAME="drive"   DESC="Drive Piezo."/>
         <METHOD NAME="enable"  DESC="Enable Piezo."/>
         <METHOD NAME="disable" DESC="Disable Piezo."/>
      </DRIVER>
   </PERIPHERAL>
</CLIP>

