<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<CLIP CATEGORY="Grove" FILE="Grove/Grove Switch.pdsclip" DESC="Grove SPDT slide Switch" FAMILY="Raspberry Pi">
   <PERIPHERAL TYPE="Grove Switch"  ELEMENT="SW1" >
      <!-- Hardware Interface -->
      <INTERFACE OBJECT="GroveSwitch">
         <PROPERTY NAME="STATE" ELEMENT="GC1" TYPE="INTEGER" GROVE="DIGITAL"/>
      </INTERFACE>
   
      <!-- Firmware Driver -->
      <DRIVER HEADER="Grove">
          <REQUIRE MODULE="GROVE"/>
          <FUNCTION DESC="Switch State">
            <RETURN TYPE="BOOLEAN"/>
         </FUNCTION>
      </DRIVER>
   </PERIPHERAL>
</CLIP>   

