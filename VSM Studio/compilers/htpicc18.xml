  <PATH ADD="%COMPILER_DIR%/BIN"/>
  <!-- File Types -->
  <FILE NAME="c" DESC="C Source" CATEGORY="Source" SOURCE="1" ICON="project_c.png"/>
  <FILE NAME="as" DESC="Assembler Source" CATEGORY="Source" SOURCE="1" ICON="project_assembly.png"/>
  <FILE NAME="h" DESC="C Include" CATEGORY="Header" SOURCE="1" ICON="project_header.png"/>
  <FILE NAME="inc" DESC="Assembler Include" CATEGORY="Header" SOURCE="1" ICON="project_source.png"/>
  <FILE NAME="lib" DESC="Object Library" CATEGORY="Library"/>
  <FILE NAME="cof" DESC="Microchip Coff" CATEGORY="Firmware Binary" FIRMWARE="1"/>
  <FILE NAME="elf" DESC="ELF" CATEGORY="Firmware Binary" FIRMWARE="1"/>
  <FILE NAME="hex" DESC="Intel Hex" CATEGORY="Firmware Binary" FIRMWARE="1"/>
  <FILE NAME="o" DESC="Object" CATEGORY="Intermediate Binary"/>
  <!-- C Compiler -->
  <PROCESS TOOL="CC" NAME="C Compiler" CMDLINE="$(CC) $(CCFLAGS) %OPTIONS% -O%OUTFILE_QUOTED% %INFILE_QUOTED%">
    <FILE INPUT="c" OUTPUT="p1"/>
    <FILE TEMP="pre"/>
    <FILE TEMP="lst"/>
    <OPTION NAME="--pass1"/>
    <OPTION NAME="--errformat=" VALUE="&quot;Error at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <OPTION NAME="--warnformat=" VALUE="&quot;Warning at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <OPTION NAME="--msgformat=" VALUE="&quot;Message at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <OPTION NAME="-D" VALUE="_XTAL_FREQ=%CLOCK%" DEFAULT="1"/>
    <MESSAGE VALUE="^(Error|Warning|Message) at file (.+) line (\d+) column (\d*): .+$">
      <FIELD NAME="FILENAME" POSITION="2"/>
      <FIELD NAME="LINE" POSITION="3" OFFSET="-1"/>
      <FIELD NAME="COLUMN" POSITION="4" OFFSET="0"/>
    </MESSAGE>
  </PROCESS>
  <!-- Assembler -->
  <PROCESS TOOL="ASM" NAME="Assembler" CMDLINE="$(ASM) $(ASMFLAGS) %OPTIONS% -O%OUTFILE_QUOTED_DOS% %INFILE_QUOTED_DOS%">
    <FILE INPUT="as" OUTPUT="obj"/>
    <FILE TEMP="rlf"/>
    <OPTION NAME="-C"/>
    <OPTION NAME="--errformat=" VALUE="&quot;Error at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <OPTION NAME="--warnformat=" VALUE="&quot;Warning at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <OPTION NAME="--msgformat=" VALUE="&quot;Message at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <MESSAGE VALUE="^(Error|Warning|Message) at file (.+) line (\d+) column (\d*): .+$">
      <FIELD NAME="FILENAME" POSITION="2"/>
      <FIELD NAME="LINE" POSITION="3" OFFSET="-1"/>
      <FIELD NAME="COLUMN" POSITION="4" OFFSET="0"/>
    </MESSAGE>
  </PROCESS>
  <!-- Linker -->
  <PROCESS TOOL="LD" NAME="Linker" CMDLINE="$(LD) $(LDFLAGS) %OPTIONS% -O%OUTFILE_QUOTED% %INFILES_QUOTED%">
    <FILE OUTPUT="cof">
      <FILE TEMP="hex"/>
      <OPTION NAME="--output=" VALUE="mcof"/>
    </FILE>
    <FILE OUTPUT="hex">
      <OPTION NAME="--output=" VALUE="intel"/>
    </FILE>
    <FILE INPUT="p1"/>
    <FILE INPUT="obj"/>
    <FILE INPUT="lib"/>
    <FILE INPUT="lpp"/>
    <FILE TEMP="hxl"/>
    <FILE TEMP="lst"/>
    <FILE TEMP="sym"/>
    <FILE TEMP="obj"/>
    <FILE TEMP="sdb"/>
    <FILE TEMP="as"/>
    <FILE TEMP="startup.obj" NAME="%OUTFILE_DIR%\startup.obj"/>
    <FILE TEMP="startup.rlf" NAME="%OUTFILE_DIR%\startup.rlf"/>
    <FILE TEMP="startup.as" NAME="%OUTFILE_DIR%\startup.as"/>
    <FILE TEMP="startup.lst" NAME="%OUTFILE_DIR%\startup.lst"/>
    <FILE TEMP="funclist." NAME="%TARGET_DIR%\funclist."/>
  </PROCESS>
  <!-- Librarian -->
  <PROCESS TOOL="LIB" NAME="Librarian" CMDLINE="$(LIB) $(LIBFLAGS) %OPTIONS% -O%OUTFILE_QUOTED% %INFILES_QUOTED%">
    <FILE OUTPUT="lib">
      <FILE INPUT="obj"/>
      <FILE INPUT="lib"/>
      <OPTION NAME="--output=" VALUE="lib"/>
    </FILE>
    <FILE OUTPUT="lpp">
      <FILE INPUT="p1"/>
      <FILE INPUT="lpp"/>
      <OPTION NAME="--output=" VALUE="lpp"/>
    </FILE>
  </PROCESS>
