  <!-- Compiler Detection -->
  <DETECT PATH="%PROGRAMFILES%/MCS Electronics/BASCOM-AVR" SUF="bascomp.exe" ARG="" STR="bascomp command line compiler"/>
  <!-- File Types -->
  <FILE NAME="bas" DESC="Basic Source" CATEGORY="Source" SOURCE="1" ICON="project_c.png" SYNTAX="BASCOM-AVR.SYN"/>
  <FILE NAME="hex" DESC="Intel Hex" CATEGORY="Firmware Binary" FIRMWARE="1"/>
  <FILE NAME="obj" DESC="AVR Studio Object File" CATEGORY="Firmware Binary" FIRMWARE="1"/>
  <FILE NAME="dbg" DESC="BASCOM-AVR Debug Information File" CATEGORY="Firmware Binary" FIRMWARE="1"/>
  <!-- Basic -->
  <PROCESS TOOL="BAS" NAME="Basic" CMDLINE="$(BAS) &quot;%INFILE_DIR%/%INFILE_NAME%.bas&quot; $(BASFLAGS) %OPTIONS%">
    <FILE OUTPUT="hex">
      <FILE INPUT="bas"/>
      <FILE TEMP="obj"/>
      <FILE TEMP="dbg"/>
      <FILE TEMP="bin"/>
      <FILE TEMP="rpt"/>
      <FILE ERROR="err"/>
    </FILE>
    <FILE OUTPUT="obj">
      <FILE INPUT="bas"/>
      <FILE INFO="dbg"/>
      <FILE TEMP="hex"/>
      <FILE TEMP="bin"/>
      <FILE TEMP="rpt"/>
      <FILE ERROR="err" NAME="%INFILE_DIR%/%INFILE_NAME%.err"/>
    </FILE>
    <!-- Files are in the source file directory -->
    <POSTPROCESS CMDLINE="@if exist %OUTFILE_NAME%.obj rm %OUTFILE_NAME%.obj"/>
    <POSTPROCESS CMDLINE="@if exist %OUTFILE_NAME%.dbg rm %OUTFILE_NAME%.dbg"/>
    <POSTPROCESS CMDLINE="@if exist %OUTFILE_NAME%.hex rm %OUTFILE_NAME%.hex"/>
    <POSTPROCESS CMDLINE="@if exist %OUTFILE_NAME%.bin rm %OUTFILE_NAME%.bin"/>
    <POSTPROCESS CMDLINE="@if exist %OUTFILE_NAME%.rpt rm %OUTFILE_NAME%.rpt"/>
    <POSTPROCESS CMDLINE="@if exist %INFILE_DIR%\%INFILE_NAME%.obj mv -f %INFILE_DIR%/%INFILE_NAME%.obj %OUTFILE_NAME%.obj"/>
    <POSTPROCESS CMDLINE="@if exist %INFILE_DIR%\%INFILE_NAME%.dbg mv -f %INFILE_DIR%/%INFILE_NAME%.dbg %OUTFILE_NAME%.dbg"/>
    <POSTPROCESS CMDLINE="@if exist %INFILE_DIR%\%INFILE_NAME%.hex mv -f %INFILE_DIR%/%INFILE_NAME%.hex %OUTFILE_NAME%.hex"/>
    <POSTPROCESS CMDLINE="@if exist %INFILE_DIR%\%INFILE_NAME%.bin mv -f %INFILE_DIR%/%INFILE_NAME%.bin %OUTFILE_NAME%.bin"/>
    <POSTPROCESS CMDLINE="@if exist %INFILE_DIR%\%INFILE_NAME%.rpt mv -f %INFILE_DIR%/%INFILE_NAME%.rpt %OUTFILE_NAME%.rpt"/>
    <MESSAGE VALUE="^Error :.+Line :.*(\d+).*, in File : (.+)$" SEVERITY="ERROR">
      <FIELD NAME="FILENAME" POSITION="2"/>
      <FIELD NAME="LINE" POSITION="1" OFFSET="-1"/>
    </MESSAGE>
  </PROCESS>
  <!-- Compilation Targets -->
  <TARGET NAME="Debug">
    <FILE OUTPUT="obj"/>
  </TARGET>
  <TARGET NAME="Release">
    <FILE OUTPUT="hex"/>
    <POSTPROCESS CMDLINE="cp &quot;%OUTFILE%&quot; &quot;%SOLUTION_ABS_DIR%/%PROJECT_NAME%.%OUTFILE_TYPE%&quot;"/>
  </TARGET>
  <SOURCE NAME="main.bas" FROM="main.bas" OPEN="1"/>
