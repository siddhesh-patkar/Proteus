    <OPTION NAME="--enable_hardware_workaround=" DESC="Generate hardware workaround for specified problem: waid[,waid[...]]" PARAM="NUMBER" ADVANCED="1"/>
    <OPTION NAME="--enable_multibytes" DESC="Enable multibyte support" ADVANCED="1"/>
