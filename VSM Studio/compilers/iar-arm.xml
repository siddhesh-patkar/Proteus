    <OPTION NAME="--simple" DESC="Save as SimpleCode" ADVANCED="1"/>
    <OPTION NAME="--bin" DESC="Save as raw binary" ADVANCED="1"/>
    <OPTION NAME="--silent" DESC="Silent operation" ADVANCED="1"/>
