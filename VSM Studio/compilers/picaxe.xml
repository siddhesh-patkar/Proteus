<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<COMPILER NAME="PICAXE" ID="PICAXE" VERSION="1.00" URL="http://www.picaxe.com/" AVAILABILITY="Internal" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schema/compiler.xsd">
  <!-- Command Processor -->
  <TOOL NAME="NONE" FILE="CMD.EXE">
  </TOOL>
  <!-- Available controllers for this compiler -->
  <FAMILY NAME="PICAXE">
    <CONTROLLER NAME="PICAXE08"/>
    <CONTROLLER NAME="PICAXE08-IC"/>
    <CONTROLLER NAME="PICAXE08M"/>
    <CONTROLLER NAME="PICAXE08M-IC"/>
    <CONTROLLER NAME="PICAXE08M2"/>
    <CONTROLLER NAME="PICAXE08M2-IC"/>
    <CONTROLLER NAME="PICAXE14M"/>
    <CONTROLLER NAME="PICAXE14M-IC"/>
    <CONTROLLER NAME="PICAXE14M2"/>
    <CONTROLLER NAME="PICAXE14M2-IC"/>
    <CONTROLLER NAME="PICAXE18"/>
    <CONTROLLER NAME="PICAXE18-IC"/>
    <CONTROLLER NAME="PICAXE18A"/>
    <CONTROLLER NAME="PICAXE18A-IC"/>
    <CONTROLLER NAME="PICAXE18M2"/>
    <CONTROLLER NAME="PICAXE18M2-IC"/>
    <CONTROLLER NAME="PICAXE18X"/>
    <CONTROLLER NAME="PICAXE18X-IC"/>
    <CONTROLLER NAME="PICAXE20M"/>
    <CONTROLLER NAME="PICAXE20M-IC"/>
    <CONTROLLER NAME="PICAXE20M2"/>
    <CONTROLLER NAME="PICAXE20M2-IC"/>
    <CONTROLLER NAME="PICAXE28"/>
    <CONTROLLER NAME="PICAXE28-IC"/>
    <CONTROLLER NAME="PICAXE28A"/>
    <CONTROLLER NAME="PICAXE28A-IC"/>
    <CONTROLLER NAME="PICAXE28X"/>
    <CONTROLLER NAME="PICAXE28X-IC"/>
    <CONTROLLER NAME="PICAXE28X1"/>
    <CONTROLLER NAME="PICAXE28X1-IC"/>
    <CONTROLLER NAME="PICAXE28X2"/>
    <CONTROLLER NAME="PICAXE28X2-IC"/>
    <CONTROLLER NAME="PICAXE40X"/>
    <CONTROLLER NAME="PICAXE40X-IC"/>
    <CONTROLLER NAME="PICAXE40X1"/>
    <CONTROLLER NAME="PICAXE40X1-IC"/>
    <CONTROLLER NAME="PICAXE40X2"/>
    <CONTROLLER NAME="PICAXE40X2-IC"/>
  </FAMILY>
  <!-- Compiler Detection -->
  <DETECT PATH="%TOOLS_DIR%/PICAXE" SUF="axe08.exe" ARG="" STR="Enhanced Compiler"/>
  <!-- File Types -->
  <FILE NAME="BAS" DESC="Basic Source" CATEGORY="Source" SOURCE="1" FIRMWARE="1" ICON="project_c.png" SYNTAX="PICAXE.SYN"/>
  <!-- Copy file -->
  <PROCESS TOOL="NONE" NAME="NONE" CMDLINE="$(NONE) /c copy &quot;%INFILE%&quot; &quot;%OUTFILE%&quot;">
    <FILE OUTPUT="BAS">
      <FILE INPUT="BAS"/>
    </FILE>
  </PROCESS>
  <!-- Compilation Targets -->
  <TARGET NAME="Debug">
    <FILE OUTPUT="BAS"/>
  </TARGET>
  <TARGET NAME="Release">
    <FILE OUTPUT="BAS"/>
  </TARGET>
  <SOURCE NAME="main.bas" FROM="picaxemain.bas" OPEN="1"/>
  <DIR NAME="WORKING" VALUE="PROJECT"/>
  <DIR NAME="TARGET" VALUE="PROJECT"/>
</COMPILER>
