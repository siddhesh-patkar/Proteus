      <PARAM VALUE="all" DESC="All"/>
      <PARAM VALUE="none" DESC="None"/>
      <PARAM VALUE="default" DESC="Default" DEFAULT="1"/>
    </OPTION>
    <OPTION NAME="--scandep" DESC="Scan for dependencies" ADVANCED="1"/>
    <OPTION NAME="--setoption=" DESC="Set advanced options" PARAM="STRING" MULTIPLE="1" ADVANCED="1"/>
