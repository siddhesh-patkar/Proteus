    <OPTION NAME="--pre" DESC="Preprocess only" ADVANCED="1"/>
    <OPTION NAME="--proto" DESC="Generate function prototypes" ADVANCED="1"/>
    <OPTION NAME="--ram=" DESC="Specify RAM ranges" PARAM="STRING" ADVANCED="1"/>
    <OPTION NAME="--rom=" DESC="Specify ROM ranges" PARAM="STRING" ADVANCED="1"/>
