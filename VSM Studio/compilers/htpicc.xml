      <PARAM VALUE="pickit3" DESC="MPLAB PICKit 3"/>
      <PARAM VALUE="realice" DESC="MPLAB REAL ICE"/>
    </OPTION>
    <OPTION NAME="--double=" DESC="Size of double type" PARAM="CHOICE" ADVANCED="1">
      <PARAM VALUE="24" DESC="Double type is 24-bits"/>
      <PARAM VALUE="32" DESC="Double type is 32-bits"/>
      <PARAM VALUE="default" DESC="Default" DEFAULT="1"/>
    </OPTION>
    <OPTION NAME="--echo" DESC="Echo command line" ADVANCED="1"/>
    <OPTION NAME="--errformat=" DESC="Error format" PARAM="STRING" ADVANCED="1"/>
    <OPTION NAME="--errors=" DESC="Maximum number of errors" PARAM="NUMBER" ADVANCED="1"/>
