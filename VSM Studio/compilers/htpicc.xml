    <OPTION NAME="-E" DESC="Redirect compile errors" PARAM="STRING" ADVANCED="1"/>
    <OPTION NAME="-G" DESC="Generate symbolic debug information" PARAM="CHOICE" ADVANCED="1">
      <PARAM VALUE="" DESC="On" DEFAULT="1"/>
      <PARAM VALUE="=off" DESC="Off"/>
    </OPTION>
    <OPTION NAME="-I" DESC="Specify include path" PARAM="STRING" MULTIPLE="1"/>
    <OPTION TOOL="LD" NAME="-L" DESC="Set linker option" PARAM="STRING" MULTIPLE="1"/>
    <OPTION NAME="-M" DESC="Generate map file" PARAM="STRING" ADVANCED="1"/>
    <OPTION NAME="-N" DESC="Identifier length" PARAM="NUMBER" ADVANCED="1"/>
    <OPTION NAME="-O" DESC="Output file" PARAM="STRING" ADVANCED="1"/>
    <OPTION NAME="-P" DESC="Preprocess assembler" ADVANCED="1"/>
    <OPTION NAME="-Q" DESC="Quiet mode" ADVANCED="1"/>
    <OPTION NAME="-S" DESC="Compile to assembler" ADVANCED="1"/>
    <OPTION NAME="-U" DESC="Undefine preprocessor symbol" PARAM="STRING" MULTIPLE="1"/>
    <OPTION NAME="-V" DESC="Verbose mode" ADVANCED="1"/>
    <OPTION NAME="-X" DESC="Strip local symbols" ADVANCED="1"/>
    <OPTION NAME="--addrqual=" DESC="Specify compiler response to address qualifiers" PARAM="CHOICE" ADVANCED="1">
      <PARAM VALUE="ignore" DESC="Bank qualifiers are ignored" DEFAULT="1"/>
      <PARAM VALUE="request" DESC="Try to locate variable in requested bank, if bank unavailable use another bank"/>
      <PARAM VALUE="require" DESC="Locate variable in required bank, if bank unavailable produce error"/>
      <PARAM VALUE="reject" DESC="Bank qualifiers are not allowed and will result in an error being produced"/>
    </OPTION>
    <OPTION NAME="--apbank=" DESC="Specify bank for autos and parameters" PARAM="NUMBER" ADVANCED="1"/>
    <OPTION NAME="--asmlist" DESC="Generate assembler listing" ADVANCED="1"/>
    <OPTION NAME="--callgraph=" DESC="Style of call graph listing in map file" PARAM="CHOICE" ADVANCED="1">
      <PARAM VALUE="std" DESC="Short form" DEFAULT="1"/>
      <PARAM VALUE="crit" DESC="Critical paths only"/>
      <PARAM VALUE="full" DESC="Fully expanded"/>
      <PARAM VALUE="none" DESC="None"/>
    </OPTION>
    <OPTION NAME="--char=" DESC="Default character type (defunct)" PARAM="CHOICE" ADVANCED="1">
      <PARAM VALUE="unsigned" DESC="Char type is unsigned"/>
      <PARAM VALUE="signed" DESC="Char type is signed"/>
      <PARAM VALUE="default" DESC="Default" DEFAULT="1"/>
    </OPTION>
    <OPTION NAME="--checksum=" DESC="Calculate a checksum and store the result in program memory" PARAM="STRING" ADVANCED="1"/>
    <OPTION NAME="--chip=" DESC="Select processor" PARAM="STRING"/>
    <OPTION NAME="--codeoffset=" DESC="Specify ROM address" PARAM="STRING" ADVANCED="1"/>
    <OPTION NAME="--cr" DESC="Generate cross reference file" PARAM="STRING" ADVANCED="1"/>
