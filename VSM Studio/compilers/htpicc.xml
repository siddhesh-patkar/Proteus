    <CONTROLLER NAME="PIC16LF726">
      <OPTION NAME="--chip=" VALUE="16LF726"/>
    </CONTROLLER>
    <CONTROLLER NAME="PIC16LF727">
      <OPTION NAME="--chip=" VALUE="16LF727"/>
    </CONTROLLER>
  </FAMILY>
  <!-- Compiler Detection -->
  <DETECT KEYLOC="HKEY_LOCAL_MACHINE\SOFTWARE\HI-TECH Software\PICC" LEVELS="3" TYPE="ANONYMOUS" KEY="InstDir" SUF="BIN/picc.exe" DATE="2010-08-09">
    <CHECK ARG="--ver" STR="HI-TECH"/>
    <CHECK ARG="--ver" STR="PIC"/>
  </DETECT>
  <DETECT PATH="%PROGRAMFILES%/HI-TECH Software/PICC/%SUB%" SUF="BIN/picc.exe" CD=".." LEVELS="3" ARG="--ver" DATE="2010-08-19">
    <CHECK ARG="--ver" STR="HI-TECH"/>
    <CHECK ARG="--ver" STR="PIC"/>
  </DETECT>
  <DETECT PATH="%PROGRAMW6432%/HI-TECH Software/PICC/%SUB%" SUF="BIN/picc.exe" CD=".." LEVELS="3" ARG="--ver" DATE="2010-08-19">
    <CHECK STR="HI-TECH"/>
    <CHECK STR="PIC"/>
  </DETECT>
  <PATH ADD="%COMPILER_DIR%/BIN"/>
  <!-- File Types -->
  <FILE NAME="c" DESC="C Source" CATEGORY="Source" SOURCE="1" ICON="project_c.png"/>
  <FILE NAME="as" DESC="Assembler Source" CATEGORY="Source" SOURCE="1" ICON="project_assembly.png"/>
  <FILE NAME="h" DESC="C Include" CATEGORY="Header" SOURCE="1" ICON="project_header.png"/>
  <FILE NAME="lib" DESC="Object Library" CATEGORY="Library"/>
  <FILE NAME="lpp" DESC="P-Code Library" CATEGORY="Library"/>
  <FILE NAME="cof" DESC="Microchip Coff" CATEGORY="Firmware Binary" FIRMWARE="1"/>
  <FILE NAME="hex" DESC="Intel Hex" CATEGORY="Firmware Binary" FIRMWARE="1"/>
  <FILE NAME="obj" DESC="Object" CATEGORY="Intermediate Binary"/>
  <FILE NAME="p1" DESC="P-Code" CATEGORY="Intermediate Binary"/>
  <!-- C Compiler -->
  <PROCESS TOOL="CC" NAME="C Compiler" CMDLINE="$(CC) $(CCFLAGS) %OPTIONS% -O%OUTFILE_QUOTED% %INFILE_QUOTED%">
    <FILE INPUT="c"/>
    <FILE OUTPUT="p1" NAME="%INFILE_NAME%.p1"/>
    <FILE TEMP="pre" NAME="%OUTFILE_DIR%\%INFILE_NAME%.pre"/>
    <FILE TEMP="lst" NAME="%OUTFILE_DIR%\%INFILE_NAME%.lst"/>
    <OPTION NAME="--pass1"/>
    <OPTION NAME="--errformat=" VALUE="&quot;Error at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <OPTION NAME="--warnformat=" VALUE="&quot;Warning at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <OPTION NAME="--msgformat=" VALUE="&quot;Message at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <OPTION NAME="-D" VALUE="_XTAL_FREQ=%CLOCK%" DEFAULT="1"/>
    <MESSAGE VALUE="^(Error|Warning|Message) at file (.+) line (\d+) column (\d*): .+$">
      <FIELD NAME="FILENAME" POSITION="2"/>
      <FIELD NAME="LINE" POSITION="3" OFFSET="-1"/>
      <FIELD NAME="COLUMN" POSITION="4" OFFSET="0"/>
    </MESSAGE>
  </PROCESS>
  <!-- Assembler -->
  <PROCESS TOOL="ASM" NAME="Assembler" CMDLINE="$(ASM) $(ASMFLAGS) %OPTIONS% -O%OUTFILE_QUOTED% %INFILE_QUOTED%">
    <FILE INPUT="as"/>
    <FILE OUTPUT="obj" NAME="%INFILE_NAME%.obj"/>
    <OPTION NAME="-C"/>
    <OPTION NAME="--errformat=" VALUE="&quot;Error at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <OPTION NAME="--warnformat=" VALUE="&quot;Warning at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <OPTION NAME="--msgformat=" VALUE="&quot;Message at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <MESSAGE VALUE="^(Error|Warning|Message) at file (.+) line (\d+) column (\d*): .+$">
      <FIELD NAME="FILENAME" POSITION="2"/>
      <FIELD NAME="LINE" POSITION="3" OFFSET="-1"/>
      <FIELD NAME="COLUMN" POSITION="4" OFFSET="0"/>
    </MESSAGE>
  </PROCESS>
  <!-- Linker -->
  <PROCESS TOOL="LD" NAME="Linker" CMDLINE="$(LD) $(LDFLAGS) %OPTIONS% -O%OUTFILE_QUOTED% %INFILES_QUOTED%">
    <FILE OUTPUT="cof">
      <FILE TEMP="hex" NAME="%OUTFILE_DIR%\%OUTFILE_NAME%.hex"/>
      <OPTION NAME="--output=" VALUE="mcof"/>
    </FILE>
    <FILE OUTPUT="hex">
      <OPTION NAME="--output=" VALUE="intel"/>
    </FILE>
    <FILE INPUT="p1"/>
    <FILE INPUT="obj"/>
    <FILE INPUT="lib"/>
    <FILE INPUT="lpp"/>
    <FILE TEMP="hxl" NAME="%OUTFILE_DIR%\%OUTFILE_NAME%.hxl"/>
    <FILE TEMP="lst" NAME="%OUTFILE_DIR%\%OUTFILE_NAME%.lst"/>
    <FILE TEMP="sym" NAME="%OUTFILE_DIR%\%OUTFILE_NAME%.sym"/>
    <FILE TEMP="obj" NAME="%OUTFILE_DIR%\%OUTFILE_NAME%.obj"/>
    <FILE TEMP="sdb" NAME="%OUTFILE_DIR%\%OUTFILE_NAME%.sdb"/>
    <FILE TEMP="as" NAME="%OUTFILE_DIR%\%OUTFILE_NAME%.as"/>
    <FILE TEMP="startup.obj" NAME="%OUTFILE_DIR%\startup.obj"/>
    <FILE TEMP="startup.rlf" NAME="%OUTFILE_DIR%\startup.rlf"/>
    <FILE TEMP="startup.as" NAME="%OUTFILE_DIR%\startup.as"/>
    <FILE TEMP="startup.lst" NAME="%OUTFILE_DIR%\startup.lst"/>
    <FILE TEMP="funclist." NAME="%TARGET_DIR%\funclist."/>
    <OPTION NAME="--errformat=" VALUE="&quot;Error at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <OPTION NAME="--warnformat=" VALUE="&quot;Warning at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
    <OPTION NAME="--msgformat=" VALUE="&quot;Message at file %%%%f line %%%%l column %%%%c: (%%%%n) %%%%s&quot;"/>
  </PROCESS>
  <!-- Librarian -->
  <PROCESS TOOL="LIB" NAME="Librarian" CMDLINE="$(LIB) $(LIBFLAGS) %OPTIONS% -O%OUTFILE_QUOTED% %INFILES_QUOTED%">
    <FILE OUTPUT="lib">
      <FILE INPUT="obj"/>
      <FILE INPUT="lib"/>
      <OPTION NAME="--output=" VALUE="lib"/>
    </FILE>
    <FILE OUTPUT="lpp">
      <FILE INPUT="p1"/>
      <FILE INPUT="lpp"/>
      <OPTION NAME="--output=" VALUE="lpp"/>
    </FILE>
  </PROCESS>
  <!-- Properties -->
  <PROPERTY NAME="CLOCK" DESC="Clock for delays" VALUE="1000000"/>
  <!-- Compilation Targets -->
  <TARGET NAME="Debug">
    <FILE OUTPUT="cof"/>
    <OPTION TOOL="CC" NAME="-G"/>
    <OPTION TOOL="ASM" NAME="-G"/>
    <OPTION TOOL="LD" NAME="-G"/>
  </TARGET>
  <TARGET NAME="Release">
    <FILE OUTPUT="hex"/>
    <POSTPROCESS CMDLINE="cp &quot;%OUTFILE%&quot; &quot;%SOLUTION_ABS_DIR%/%PROJECT_NAME%.%OUTFILE_TYPE%&quot;"/>
  </TARGET>
  <SOURCE NAME="main.c" FROM="htpiccmain.c" OPEN="1"/>
</COMPILER>
